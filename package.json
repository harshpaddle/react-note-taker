  },
  "homepage": "https://github.com/harshpaddle/react-note-taker#readme",
  "dependencies": {
    "express": "^4.17.3",
    "if-env": "^1.0.4",
    "mongoose": "^5.5.13",
    "morgan": "^1.9.1"
  }
}
